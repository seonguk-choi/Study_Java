package pack03_Extends;

public class SmartCellPhone extends DmbCellPhone{
	String internetSpd;
	public SmartCellPhone(String spd) {
		
	}
	
	void turnOninternet(String spd) {
		System.out.println(spd + " 속도로 인터넷을 시작합니다.");
	}
	void turnOnyoutube() {
		System.out.println("유투브를 시작합니다.");
	}
	void turnOffinternet(String spd) {
		System.out.println("인터넷을 종료합니다.");
	}
	void turnOffyoutube() {
		System.out.println("유투브를 종료합니다.");
	}
}
