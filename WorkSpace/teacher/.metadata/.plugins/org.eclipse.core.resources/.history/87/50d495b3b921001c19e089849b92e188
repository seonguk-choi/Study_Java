package pack02_Array;

public class Ex07_sort {
	public static void main(String[] args) {
		int a = 10;
		int b = 20; 
		System.out.println("변경 전 a의 값 : " +  a + " b의값 :" + b);

		System.out.println("변경 전 a의 값 : " +  20 + " b의값 :" + 10);
		
		//swap
		//a와b에 직접 값을 할당하지 않고. (변수사용은 가능)
		//두수의 값을 바꿔보세요.
	}
}
