package pack02_Array;

public class Ex01_Array {
	public static void main(String[] args) {
		//String , 배열 == ( 참조 타입 )
		int a = 10;
		int b = 20;
		int c = 30;
		//100.... ? 
		//int[] array = < 100
		//배열의 선언 
		//데이터타입[] 배열의 이름 ;
		//배열의 크기를 할당
		//배열의이름 = new 데이터타입[크기int로 넣어줌];
		int[] array ;//<선언
		array = new int[3];//배열의 크기 할당.
		System.out.println(array);
		
		
	}
}
