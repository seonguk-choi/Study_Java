package pack01_if;

public class Ex04_if_if {
	public static void main(String[] args) {
		//제어문 - 중첩이 가능하다.
		//1.조건문 , 2.반복문 
		//중첩이가능하다는말은 블럭킹 내부에서 또 제어문을 만들수있다.
		int score = 90;
		if(score >= 90) {
			System.out.println("90이랑 같거나 크다");
			if(score >= 80) {
				System.out.println("80보다도 크다");
			}//if
		}//if
	}
}
