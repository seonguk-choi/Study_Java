package pack02_Array;

public class Ex04_2Array {
	public static void main(String[] args) {
		//1차원 3: [][][]
		//2차원 3 2 :[][][]
		//          [][][]
		//2차원 배열의 선언. 데이터타입 [][] 배열의이름;
		int[][] arr ;//<-int형 2차원 배열선언
		arr = new int[2][3];// 행 * 열 = 6 , [4] [3] = 12
		System.out.println(arr.length);
	}
}
